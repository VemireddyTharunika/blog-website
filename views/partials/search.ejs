<div class="searchBar">
    <div class="container">
        <from action="/search" class="search__form" role="search" method="POST">

            <input
            type="search"
            aria-Label="Search"
            id="searchInput"
            name="searchTerm"
            placeholder="search the site..."/>
        </from>
        <div id="searchClose">Close</div>
    </div>
</div>